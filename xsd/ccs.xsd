<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2011 rel. 2 (http://www.altova.com) by qq (qq) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="UID">
		<xs:annotation>
			<xs:documentation>Серийный номер чипа бесконтактной карты.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:hexBinary">
			<xs:pattern value="[0-9a-fA-F]{8}|[0-9a-fA-F]{14}|[0-9a-fA-F]{20}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Version">
		<xs:annotation>
			<xs:documentation>Версия.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedByte">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="255"/>
			<xs:pattern value="\d+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CampusCard" final="restriction">
		<xs:annotation>
			<xs:documentation>Кампусный номер.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="PIN">
		<xs:annotation>
			<xs:documentation>PIN держателя карты.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\d{4}\d{0,8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OperationType">
		<xs:annotation>
			<xs:documentation>Тип финансовой операции.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="debit"/>
			<xs:enumeration value="credit"/>
			<xs:enumeration value="withdraw"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Amount">
		<xs:restriction base="xs:unsignedInt">
			<xs:maxInclusive value="16777215"/>
			<xs:pattern value="\d+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Signature">
		<xs:annotation>
			<xs:documentation>Подпись файла, расчитанная на основе ключа. Рассчитывается хэш на все аттрибуты xml-файа и шифруется на ключе указанном в Key_Attribute. Результат записывается в значение элемента Signature</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:hexBinary">
					<xs:attribute name="KeyVersion" use="required">
						<xs:annotation>
							<xs:documentation>Номер  версии ключа.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:minInclusive value="1"/>
								<xs:maxInclusive value="255"/>
								<xs:pattern value="\d+"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Request">
		<xs:annotation>
			<xs:documentation>Запрос на выполнение операции.от внешнего софта к службе</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="Perso">
					<xs:annotation>
						<xs:documentation>Персонализация карты с переводом ее в активное состояние.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="UID" type="UID" use="required">
							<xs:annotation>
								<xs:documentation>Серийный номер чипа персонализируемой карты</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CampusCard" type="CampusCard" use="required">
							<xs:annotation>
								<xs:documentation>Кампусный номер.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="ExpirationDate" use="required">
							<xs:annotation>
								<xs:documentation>Дата, до которой действует карта.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:date">
									<xs:pattern value="\d{4}-\d{2}-\d{2}"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="MappingVersion" type="Version" use="required">
							<xs:annotation>
								<xs:documentation>Версия разметки приложения.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="KeysetVersion" type="Version" use="required">
							<xs:annotation>
								<xs:documentation>Версия набора ключей, используемых в приложении.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="PIN" type="PIN" use="optional">
							<xs:annotation>
								<xs:documentation>PIN владельца карты. Если отсутствует - в приложение не пишется.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Read">
					<xs:annotation>
						<xs:documentation>Чтение с карты полного набора данных.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="PIN" use="optional">
							<xs:annotation>
								<xs:documentation>PIN держателя карты. Если отсутствует - то не проверяется.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="VerifyPin">
					<xs:annotation>
						<xs:documentation>Проверка PIN держателя карты. Предварительно требуется выполнить запрос Read.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="PIN" type="PIN" use="required">
							<xs:annotation>
								<xs:documentation>PIN держателя карты.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="ChangePin">
					<xs:annotation>
						<xs:documentation>Замена PIN держателя карты. Предварительно требуется выполнить запрос Read.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="PIN" type="PIN" use="required">
							<xs:annotation>
								<xs:documentation>PIN держателя карты.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Debit">
					<xs:annotation>
						<xs:documentation>Выполнение операции дебетования (уменьшения баланса) карты. Предварительно требуется выполнить запрос Read.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="PIN" use="optional">
							<xs:annotation>
								<xs:documentation>PIN держателя карты. Если отсутствует - то не проверяется.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="Timestamp" type="xs:dateTime" use="required">
							<xs:annotation>
								<xs:documentation>Дата и время выполнения операции.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="OperationNumber" use="required">
							<xs:annotation>
								<xs:documentation>Номер операции на устройстве приема карт. Пишется на карту</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:unsignedShort">
									<xs:pattern value="\d+"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Amount" use="required">
							<xs:annotation>
								<xs:documentation>Сумма операции в копейках.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:unsignedInt">
									<xs:maxInclusive value="16777215"/>
									<xs:pattern value="\d+"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Credit">
					<xs:annotation>
						<xs:documentation>Выполнение операции кредитования  (увеличение баланса) карты. Предварительно требуется выполнить запрос Read.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="PIN" use="optional">
							<xs:annotation>
								<xs:documentation>PIN держателя карты. Если отсутствует - то не проверяется.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="Timestamp" type="xs:dateTime" use="required">
							<xs:annotation>
								<xs:documentation>Дата и время выполнения операции.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="OperationNumber" use="required">
							<xs:annotation>
								<xs:documentation>Номер операции на устройстве приема карт. Пишется на карту</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:unsignedShort">
									<xs:pattern value="\d+"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Amount" use="required">
							<xs:annotation>
								<xs:documentation>Сумма операции в копейках.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:unsignedInt">
									<xs:maxInclusive value="16777215"/>
									<xs:pattern value="\d+"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="CalculateSignature">
					<xs:annotation>
						<xs:documentation>Вычисление подписи на данные. Если присутствует атрибут path, подписывается содержимое файла, расположенного по пути, указанном в этом атрибуте. В противном случае подписываются данные тега. Предварительно требуется выполнить запрос Read.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attribute name="Path">
									<xs:annotation>
										<xs:documentation>Путь к файлу данных, который необходимо подписать.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="VerifySignature">
					<xs:annotation>
						<xs:documentation>Проверка подписи на данные. Если присутствует атрибут path, проверяется подпись на содержимое файла, расположенного по пути, указанном в этом атрибуте. В противном случае проверяется подпись на данные тега. Предварительно требуется выполнить запрос Read.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attribute name="Path">
									<xs:annotation>
										<xs:documentation>Путь к файлу данных, который необходимо подписать.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Signature" type="xs:hexBinary" use="required">
									<xs:annotation>
										<xs:documentation>Проверяемая подпись.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="CalculateXMLSignature">
					<xs:annotation>
						<xs:documentation>Вычисление подписи для XML файла. Программа сформирует элемент Signature внутри файла. Если отсутсвует  атрибут path или файл, то будет ошибка</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attribute name="Path" use="required">
									<xs:annotation>
										<xs:documentation>Путь к XML файлу данных, в котором надо сформировать подпись.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="VerifyXMLSignature">
					<xs:annotation>
						<xs:documentation>Проверка подписи для XML файла. Программа прочитает  элемент Signature внутри XML файла, и сравнит его с вновь вычисленной подписью и сообщит результат. Если отсутсвует  атрибут path или файл, то будет ошибка</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attribute name="Path" use="required">
									<xs:annotation>
										<xs:documentation>Путь к XML файлу данных, в котором надо проверить существующую подпись</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="RequestData">
					<xs:annotation>
						<xs:documentation>Запрос к ССПД на получение данных о держателе карты.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="CampusCard" type="CampusCard" use="required">
							<xs:annotation>
								<xs:documentation>Кампусный номер.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="RequestPerso">
					<xs:annotation>
						<xs:documentation>Запрос к ССПД на персонализацию.</xs:documentation>
					</xs:annotation>
					<xs:complexType/>
				</xs:element>
				<xs:element name="MessagePerso">
					<xs:annotation>
						<xs:documentation>Сообщение в ССПД о факте персонализации.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="UID" type="UID" use="required">
							<xs:annotation>
								<xs:documentation>Серийный номер чипа персонализируемой карты</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CampusCard" type="CampusCard" use="required">
							<xs:annotation>
								<xs:documentation>Кампусный номер.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="SetSamReader">
					<xs:annotation>
						<xs:documentation>Считыватель контактных карт, который будет использован для работы с SAM модулем.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Type" use="required">
							<xs:annotation>
								<xs:documentation>Тип считывателя.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="PC/SC"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Device" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>Имя устройства считывателя. 
Для считывателей, подключаемых посредством службы Смарт-карты  это имя, возвращаемое функцией SCardListReaders().</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="SetContactlessReader">
					<xs:annotation>
						<xs:documentation>Считываетель бесконтактных карт, который будет использован для работы с кампусными картами.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Type" use="required">
							<xs:annotation>
								<xs:documentation>Тип считывателя.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="OMNIKEY CardMan 5x21"/>
									<xs:enumeration value="ACS ACR128U"/>
									<xs:enumeration value="ACS ACR1281U"/>
									<xs:enumeration value="SCM SDI01x"/>
									<xs:enumeration value="VivoPay Kiosk II (HID)"/>
									<xs:enumeration value="VivoPay Kiosk II (COM)"/>
									<xs:enumeration value="ACS ACR122U"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Device" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>Имя устройства считывателя.
Для считывателей, подключаемых посредством службы Смарт-карты  это имя, возвращаемое функцией SCardListReaders().
Для считываетлей подключаемых через COM порт это имя COM порта.
Для считывателей, подключаемых как HID устройства это полный путь к HID устройству.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="SetImportDirectory">
					<xs:annotation>
						<xs:documentation>Каталог, в котором будет осуществляться поиск файлов параметров и стоп-листов.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Path" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>Путь к каталогу с файлами параметров и стоп-листов.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="SetExportDirectory">
					<xs:annotation>
						<xs:documentation>Каталог, в которй будут сохраняться файлы журналов.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Path" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>Путь к каталогу с файлами журналов.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="SetServerParams">
					<xs:annotation>
						<xs:documentation>Параметры подключения к серверу ССПД.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Host" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>Имя хоста или IP адрес хоста, на котором запущен сервер.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="Port" use="required">
							<xs:annotation>
								<xs:documentation>Порт, на котором сервер ожидает входящие подключения.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:unsignedShort">
									<xs:minInclusive value="1"/>
									<xs:pattern value="\d+"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="UriTemplate" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>Шаблон URI, с помощью которого будут производиться обращения к серверу. В том месте шаблона, где должны быть данные, следует поместить строку '%s'.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="SetAuthorizParams">
					<xs:annotation>
						<xs:documentation>Параметры подключения к серверу авторизации.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Host" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>Имя хоста или IP адрес хоста, на котором запущен сервер.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="Port" use="required">
							<xs:annotation>
								<xs:documentation>Порт, на котором сервер ожидает входящие подключения.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:unsignedShort">
									<xs:minInclusive value="1"/>
									<xs:pattern value="\d+"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="UriTemplate" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>Шаблон URI, с помощью которого будут производиться обращения к серверу. В том месте шаблона, где должны быть данные, следует поместить строку '%s'.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="GetSamReaderStatus">
					<xs:annotation>
						<xs:documentation>Получение состояния считывателя SAM модуля и самого SAM модуля.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="GetContactlessReaderStatus">
					<xs:annotation>
						<xs:documentation>Получение состояния считывателя кампусных карт и наличия карты в поле считывателя.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="GetParamsStatus">
					<xs:annotation>
						<xs:documentation>Получение информации о файле параметров, используемого службой.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="GetStoplistStatus">
					<xs:annotation>
						<xs:documentation>Получение информации о файле стоп-листа, используемого службой.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="GetServerStatus">
					<xs:annotation>
						<xs:documentation>Проверка связи с сервером ССПД.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="GetAvailableRequests">
					<xs:annotation>
						<xs:documentation>Получение списка доступных запросов.</xs:documentation>
					</xs:annotation>
